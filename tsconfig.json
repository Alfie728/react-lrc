{
  "compilerOptions": {
    "lib": ["DOM", "ES2015"],
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "outDir": "build",
    "strictNullChecks": true,
    "target": "ES5",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "downlevelIteration": true
  },
  "exclude": ["build", "examples"]
}
